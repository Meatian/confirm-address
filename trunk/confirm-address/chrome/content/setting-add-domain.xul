<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/global.css" type="text/css"?>
<!DOCTYPE window SYSTEM "chrome://confirm-address/locale/confirm-address.dtd">

	<dialog id="setting" title="&setting.sub-dialog.title;"
	        xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
	        buttons="accept,cancel"
	        onload="startup()"
	        ondialogaccept="return doOK();"
	        ondialogcancel="return doCancel();">

		<script type="application/x-javascript" src="chrome://confirm-address/content/setting-add-domain.js"/>
		
		<label value="&setting.sub-dialog.message;"/>
		<textbox id="textbox"/>
	</dialog>
